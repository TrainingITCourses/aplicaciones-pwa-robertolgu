{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
    "dataGroups":[
      {
        "name": "cache-first",
        "urls":[
          "http://127.0.0.1:8080/assets/data/agencies.json",
          "http://127.0.0.1:8080/assets/data/missiontypes.json",
          "http://127.0.0.1:8080/assets/data/launchstatus.json"
        ],
        "cacheConfig":{
          "strategy":"performance",
          "maxSize":10,
          "maxAge":"7d"
        }
      },
      {
        "name": "api-first",
        "urls":[
          "https://launchlibrary.net/1.4/launch/1950-01-01?limit=2000"
        ],
        "cacheConfig":{
          "strategy" : "freshness",
          "maxSize": 2000,
          "maxAge" : "1d",
          "timeout" : "3s"
        }
      }

    ]
}
